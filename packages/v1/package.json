{
  "name": "@jpyc/sdk-v1-examples",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "description": "Code examples for JPYC v1 SDK",
  "repository": "https://github.com/jcam1/sdk-examples.git",
  "homepage": "https://github.com/jcam1/sdk-examples.git#readme",
  "engines": {
    "node": "^20.12.0",
    "yarn": "^1.22.22"
  },
  "scripts": {
    "env": "cp .env.example .env",
    "lint": "eslint --fix ./src",
    "lint:dry-run": "eslint ./src",
    "format": "prettier --write .",
    "format:dry-run": "prettier --check .",
    "compile": "tsc",
    "compile:contracts": "hardhat compile",
    "clean": "hardhat clean",
    "node": "hardhat node",
    "deploy": "rm -f -r ignition/deployments && hardhat run ./scripts/deployments.ts --network localhost",
    "mint": "hardhat run ./src/mint.ts --network localhost",
    "total-supply": "hardhat run ./src/total-supply.ts --network localhost",
    "transfer": "hardhat run ./src/transfer.ts --network localhost",
    "approve": "hardhat run ./src/approve.ts --network localhost",
    "permit": "hardhat run ./src/permit.ts --network localhost",
    "transfer-from": "hardhat run ./src/transfer-from.ts --network localhost",
    "transfer-with-authorization": "hardhat run ./src/transfer-with-authorization.ts --network localhost",
    "receive-with-authorization": "hardhat run ./src/receive-with-authorization.ts --network localhost",
    "cancel-authorization": "hardhat run ./src/cancel-authorization.ts --network localhost"
  },
  "dependencies": {
    "@jpyc/sdk-v1": "^1.2.2",
    "crypto": "1.0.1",
    "dotenv": "16.4.5",
    "soltypes": "2.0.0",
    "viem": "2.20.0"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-ignition": "0.15.5",
    "@nomicfoundation/hardhat-ignition-viem": "0.15.5",
    "@nomicfoundation/hardhat-toolbox": "5.0.0",
    "@nomicfoundation/hardhat-toolbox-viem": "3.0.0",
    "@nomicfoundation/hardhat-verify": "2.0.9",
    "@nomicfoundation/hardhat-viem": "2.0.3",
    "@nomicfoundation/ignition-core": "0.15.5",
    "@types/node": "20.14.9",
    "@typescript-eslint/eslint-plugin": "7.15.0",
    "@typescript-eslint/parser": "7.15.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "hardhat": "2.22.8",
    "hardhat-gas-reporter": "2.2.1",
    "prettier": "3.3.2",
    "ts-node": "10.9.2",
    "typescript": "5.5.3"
  }
}
